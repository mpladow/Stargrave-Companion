{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
	  "strict": true,
	  "baseUrl": ".",
	  "paths": {
		"@components/*": ["src/components/*"],
		"@utils/*": ["src/utils/*"],
		"@context/*": ["src/context/*"],
		"@hooks/*": ["src/hooks/*"],
		"@navigation/*": ["src/Navigation/*"]
	  }
	},
	"exclude": ["node_modules"]
  }
  